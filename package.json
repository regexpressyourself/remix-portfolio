{
  "type": "module",
  "scripts": {
    "dev": "concurrently --names \"SASS ,REMIX\" -c \"green.bold,magenta.bold\" \"npm run sass:watch\" \"npm run serve:dev\"",
    "build": "remix build",
    "start": "cross-env NODE_ENV=production node server.mjs",
    "sass": "sass styles:app/styles",
    "sass:watch": "sass --watch styles:app/styles",
    "routes": "remix routes",
    "typecheck": "tsc",
    "serve:dev": "remix dev -c \"node server.mjs\""
  },
  "dependencies": {
    "@remix-run/express": "^2.1.0",
    "@remix-run/node": "^2.1.0",
    "@remix-run/react": "^2.1.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "html-react-parser": "^5.0.6",
    "isbot": "latest",
    "prettier": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "shrink-ray-current": "^4.1.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.1.0",
    "@types/react": "^18.2.36",
    "concurrently": "^7.0.0",
    "prettier-plugin-organize-imports": "^3.2.2"
  }
}
